<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android" android:layout_width="match_parent"
    android:background="@color/row_par"
    android:id="@+id/linearLayoutViewAdapater"
    android:layout_height="wrap_content">

    <ImageView
        android:id="@+id/iconFurnizor"
       android:layout_marginTop="10dp"
        android:layout_width="25dp"
        android:layout_marginRight="5dp"
        android:layout_height="15dp"
        android:layout_weight="1"
        android:contentDescription="Nu exista poza"></ImageView>

    <TextView
        android:id="@+id/tvAdapterDenumireFactura"
        android:layout_width="95dp"
        android:layout_height="wrap_content"
        android:layout_marginRight="2dp"
        android:layout_weight="1"
        android:text="DenumireFactura"
        android:textSize="20sp"></TextView>
    <TextView
        android:layout_width="5dp"
        android:id="@+id/tvPretAdapter"
        android:layout_weight="1"
        android:layout_marginRight="8dp"
        android:textSize="20sp"
        android:layout_height="25dp">
    </TextView>
    <TextView

        android:id="@+id/tvDataScadentaAdapater"
        android:layout_weight="1"
        android:textSize="20sp"
        android:layout_marginRight="5dp"
        android:layout_width="38dp"
        android:layout_height="25dp"

      ></TextView>

    <CheckBox
        android:id="@+id/cbPlatitAdapater"
        android:layout_width="5dp"

        android:layout_height="wrap_content"
        android:layout_weight="1"></CheckBox>


</LinearLayout>